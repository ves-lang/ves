mut a;
@first: loop { 
    mut b;
    @second: loop {
        mut c;
        continue @first;
        break @second;
    }
    @third: loop {
        mut d;
        break @first;
        continue @third;
    }
}

%output

|0000| PushNone
|0001| SetGlobal(0)    // mut a = none
|0002| PushNone        // mut b = none 
|0003| PushNone        // mut c = none
|0004| PopN(2)         // pop c and b
|0005| Jump(2)         // continue         <@first: start>
|0006| Pop             // pop c
|0007| Jump(10)        // break            <@second: end> + 1
|0008| Pop             // pop c
|0009| Jump(3)         // continue         <@second: start>
|0010| PushNone        // mut d = none
|0011| PopN(2)         // pop d and b
|0012| Jump(19)        // break            <@first: end> + 1
|0013| Pop             // pop d
|0014| Jump(10)        // continue         <@third: start>
|0015| Pop             // pop d
|0016| Jump(10)        // continue         <@third: start>
|0017| Pop             // pop b
|0018| Jump(2)         // continue         <@first: start>
