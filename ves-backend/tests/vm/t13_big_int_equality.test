print("case: ==")
print(0 == 1 - 1, true);
print(0n == 1 - 1, true);
print(0 == 1n - 1, true);
print(0 == 1 - 1n, true);
print(0n == 1n - 1n, true);

print("case: !=")
print(1n != 1n + 1n, true);
print(1 != 1 + 1n, true);
print(1n != 1n, false);
print(1 != 2 - 1n, false);

print("case: < and <=")
print(1 < 1 + 1n, true);
print(1 <= 1 + 1n, true);

print("case: > and >=")
print(2n > 1 + 1, false)
print(2n >= 1 + 1, true)

