struct Vec3(x = 0, y = 0, z = 0) {
    @add(self, other) => Vec3(self.x + other.x, self.y + other.y, self.z + other.z)
}

let v0 = Vec3(2);
print(v0);
let v1 = Vec3(2, 3, 4);
print(v1);

print(v0 + v1);

%output

Vec3 { x: 2, y: 0, z: 0, add: <fn Vec3.@add>, init: <fn Vec3.init> }
Vec3 { x: 2, y: 3, z: 4, add: <fn Vec3.@add>, init: <fn Vec3.init> }
Vec3 { x: 4, y: 3, z: 4, add: <fn Vec3.@add>, init: <fn Vec3.init> }