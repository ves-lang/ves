mut a = 5

while a {
    a -= 1
    break    
}

print(a)

%output

|0000| PushNum32(5.0)     // push 5
|0001| SetGlobal(0)       // a = 5
// start:                 
|0002| GetGlobal(0)       // get a
|0003| JumpIfFalse(12)    // jump
|0004| Pop                // pop the condition
|0005| GetGlobal(0)       // get a
|0006| PushNum32(1.0)     // push 1
|0007| Subtract           // a - 1
|0008| SetGlobal(0)       // a = a - 1
|0009| Pop                // pop the assignment result
|0010| Jump(13)           // jump to @exit
|0011| Jump(2)            // (unreachable) jump to @start
// exit:
|0012| Pop                // pop the condition
// end:
|0013| GetGlobal(0)       // get a
|0014| Print              // print a
