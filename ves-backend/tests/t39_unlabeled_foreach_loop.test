for i in 0..10, 2 {
    print(i)
}

%output

// @loop.pre_start:
|0000| PushNum32(0.0)   // i
|0001| PushNum32(10.0)  // [[END]]
|0002| PushNum32(2.0)   // [[STEP]]
// @loop.start:
|0003| GetLocal(0)      // get i
|0004| GetLocal(1)      // get [[END]]
|0005| LessThan         // i < [[END]]
|0006| JumpIfFalse(18)  // jump_if_false @loop.exit
|0007| Pop              // pop result of condition
|0008| Jump(15)         // jump @loop.body
// @loop.latch:
|0009| GetLocal(0)      // get i
|0010| GetLocal(2)      // get [[STEP]]
|0011| Add              // i + [[STEP]]
|0012| SetLocal(0)      // set i
|0013| Pop              // pop (i + [[STEP]])
|0014| Jump(3)          // jump @loop.start
// @loop.body:
|0015| GetLocal(0)      // get i
|0016| Print            // print(i)
|0017| Jump(9)          // jump @loop.latch
// @loop.exit:
|0018| Pop              // pop result of condition
|0019| PopN(3)          // pop i, [[END]], [[STEP]]
// @loop.end: