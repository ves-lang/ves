loop {
    break
    continue
}

@label: loop { 
    break @label
    continue @label
}

%output

Loop
  body: StmtKind::Block
    statements=
      StmtKind::Break
        label: None
      StmtKind::Continue
        label: None
  label: None
Loop
  body: StmtKind::Block
    statements=
      StmtKind::Break
        label: "@label"
      StmtKind::Continue
        label: "@label"
  label: "@label"
